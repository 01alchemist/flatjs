/* -*- mode: javascript -*- */

load("../libflatjs.js");
var ab = new ArrayBuffer(1024);
FlatJS.init(ab, true);

@flatjs class TSTest {
    virtual testMethod(SELF, x:number, y:string, ...z): void {
	return x + y + z.join(",");
    }
}

@flatjs class TSTest2 extends TSTest {
    virtual testMethod(SELF, x:number, y:string, ...z): void {
	return x + y + z.join(",");
    }
}
